<div class="row" href="/public/css/site.css">
    <div class="col-md-12">
        <h2>Images</h2>
        <hr />
    </div>
    <div class="col-md-4">
        {{#each data}}
        <img src="/images/uploaded/{{this}}" class="img-responsive img-thumbnail" />
        {{else}}
        <div class="col-md-12 text-center">
            <strong>No Images Available</strong>
        </div>
        {{/each}}
    </div>
</div>